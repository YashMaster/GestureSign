<?xml version="1.0" encoding="utf-8" ?>
<Language DisplayName="中文" Culture="zh-CN" Version="6.1">
  <CorePlugins>
    <ActivateWindow>
      <Name>激活窗口</Name>
      <Description>使指定窗口前置</Description>
      <ClassName>窗口类名</ClassName>
      <WindowTitle>窗口名</WindowTitle>
      <UsingRegularExpression>使用正则表达式</UsingRegularExpression>
      <Timeout>等待窗口时间</Timeout>
      <Millisecond>毫秒</Millisecond>
    </ActivateWindow>
    <Delay>
      <Name>延时</Name>
      <Description>延迟 {0} 毫秒</Description>
      <Millisecond>毫秒</Millisecond>
    </Delay>
    <HotKey>
      <Name>发送快捷键</Name>
      <Category>键盘</Category>
      <Description>发送快捷键组合到程序</Description>
      <SpecificDescription>发送快捷键 ({0})</SpecificDescription>
      <Tip>快捷键（请用键盘录入快捷键）</Tip>
      <Reset>重置</Reset>
      <AddExtraKey>(点击此处添加一个功能键)</AddExtraKey>
      <SendByKeybdEvent>用旧方式发送</SendByKeybdEvent>
