      <Name>发送消息指令</Name>
      <Category>消息</Category>
      <DescriptionTemplate>{0}到{1}</DescriptionTemplate>
      <TargetWindow>目标窗口</TargetWindow>
      <SpecificWindow>指定窗口</SpecificWindow>
      <CurrentWindow>当前窗口</CurrentWindow>
      <WindowClass>窗口类</WindowClass>
      <WindowTitle>标题</WindowTitle>
      <RegEx>使用通配符</RegEx>
      <MessageContent>消息内容</MessageContent>
      <HotKey>发送快捷键</HotKey>
      <Custom>自定义消息</Custom>
      <Method>发送方式</Method>
      <PostMessage>PostMessage</PostMessage>
      <SendMessage>SendMessage</SendMessage>
      <MessageID>消息标识符</MessageID>
    </SendMessage>
